		"lib": ["ESNext", "DOM"],
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "types": ["vite/client", "vite-plugin-svgr/client"]
	},
	"include": ["src", "global.d.ts"],
  "exclude": ["node_modules", "dist"]
}
